{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "name": "onnxruntime",
    "version-string": "1.23.1",
    "description": "Custom CPU-only, static build of onnxruntime.",
    "homepage": "https://github.com/microsoft/onnxruntime",
    "license": "MIT",
    "supports": "linux",
    "default-features": [
        "static-cpu"
    ],
    "features": {
        "static-cpu": {
            "description": "Builds a static, CPU-only version of ONNX Runtime.",
            "dependencies": [
                "protobuf",
                "abseil",
                "flatbuffers",
                "gtest",
                "re2",
                "date",
                "nlohmann-json",
                "eigen3",
                "onnx",
                "boost-mp11",
                "boost-assert",
                "boost-core",
                "cpuinfo",
                "ms-gsl",
                "safeint"
            ]
        }
    }
}
